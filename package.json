{
  "name": "homesearch",
  "version": "1.0.0",
  "description": "Property tracking app for home buying process",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
